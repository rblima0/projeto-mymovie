
.sidebarnav

.sidebarnav__item
  display flex
  padding-top 0
  padding-bottom 0

.sidebarnav__item__button
  color blue
  padding 10px 8px
  justify-content flex-start
  text-transform none
  letter-spacing 0
  width: 100%
  font-weight 500

.sidebarnav__item__icon
  display flex
  width 24px
  height 24px
  color blue
  align-items center
  margin-right 20px

.sidebarnav__item__active
  color green
  font-weight 500
